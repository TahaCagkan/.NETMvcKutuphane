@using MvcKutuphane.Models.Entity;
@model List<TBLKITAP>
@{
    ViewBag.Title = "YazarKitaplar";
    Layout = "~/Views/Shared/_LayoutYeni.cshtml";
}

<h2>Kitap Yazarı : @ViewBag.y1</h2>
<hr />
<br />

<table class="table table-striped">
    <tr>
        <th>ID</th>
        <th>Kitap Adı</th>
        <th>Kategori</th>
        <th>Yayınevi</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.AD</td>
            <td>@item.TBLKATEGORI.AD</td>
            <td>@item.YAYINEVI</td>
        </tr>
    }
</table>
