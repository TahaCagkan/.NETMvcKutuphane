@using MvcKutuphane.Models.Entity
@model List<TBLMESAJLAR>

@{
                /**/

                ViewBag.Title = "Giden";
                Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}

<h2>Giden</h2>
<hr />
<br />
<table class="table table-bordered">
    <tr>
        <th>Kime</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>İşlem</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ALICI</td>
            <td>@item.KONU</td>
            <td>@item.ICERIK</td>
            <td>@item.TARIH</td>
        </tr>
    }
</table>


